{
    "root": true,
    "extends": [
        "@mitsue",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "arrow-body-style": 0,
        "multiline-comment-style": 0,
        "@typescript-eslint/no-var-requires": 0,
        "no-console": 1
    }
}

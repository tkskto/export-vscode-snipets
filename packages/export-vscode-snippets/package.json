{
    "name": "export-vscode-snippets",
    "version": "0.0.1",
    "description": "export-vscode-snippets is a tool for export vs code snippets JSON from code.",
    "type": "module",
    "bin": {
        "vca": "bin/export.js"
    },
    "scripts": {
        "typescript": "tsc -p tsconfig.json",
        "rollup": "rollup --config",
        "build": "run-s typescript rollup",
        "test": "npm run test:base",
        "test:base": "jest --config=jest.config.js --testTimeout=60000",
        "tmp": "node bin/export.js -c .export-snippets.config.js"
    },
    "keywords": [
        "vscode",
        "export-vscode-snippets"
    ],
    "author": "Takeshi Kato <tkskto@gmail.com> (https://github.com/tkskto)",
    "license": "MIT",
    "dependencies": {
        "commander": "9.4.1"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "23.0.2",
        "@rollup/plugin-json": "5.0.1",
        "@rollup/plugin-node-resolve": "15.0.1",
        "@rollup/plugin-typescript": "9.0.2",
        "@types/jest": "29.2.0",
        "@types/node": "18.11.3",
        "jest": "29.2.1",
        "npm-run-all": "4.1.5",
        "nyc": "15.1.0",
        "rollup": "3.2.3",
        "ts-jest": "29.0.3",
        "ts-node": "10.9.1",
        "typescript": "4.8.4"
    }
}
